<div class="container container-table" style=" text-align: center; margin: 0 auto; background: rgba(54, 25, 25, .5);   margin:auto;width:30%;text-align:center;" >
  <didv class="content" >
    <% @past_events.each do |upcoming_event| %>
      <hr class="devidepost">
        <div class="upcoming_title" style="background-color:white; font-weight:bold">
          <%= upcoming_event.title %>
        </div>
      </br>
        <div class="upcomings_body">
          <%= upcoming_event.body %>
        </div>
        </br>
          <div class="starting_date_class"><%= upcoming_event.starting_date.strftime("Started At:  %Y-%m-%d %H:%M:%S") %></div>
        </br>
          <div class="end_date_class"><%= upcoming_event.end_date.strftime("Ends At:  %Y-%m-%d %H:%M:%S") %></div>
        </br>
        <div class="img img1.imgAlbum" >
          <% upcoming_event.pictures.each do |pic, remov| %>
            <%= image_tag(pic.image.url(:medium)) if pic.image? %>
          <% end %>
        </div>
    <% end %>
  </div>
</div>
